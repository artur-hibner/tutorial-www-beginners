{ 
  "gitbook": "2.x.x",
  "styles": {
    "website": "styles/website.css"
  },
  "plugins": [
    "callouts",
    "addcssjs"
    ],
  "pluginsConfig": {
    "callouts": {
      "exercise": {
        "alert": "success",
        "picto": "fa-pencil"
      },
      "important": {
        "alert": "warning",
        "picto": "fa-lightbulb-o"
      }
    },
    "addcssjs": {
      "css": ["styles/website.css"]
    }
  }
}